<div class="row">
        {{model.production.name}}
        <tbody>

            <div class="panel-body">
                <table class="table table-striped table-condensed">
                    <thead>
                    <tr>
                        <th class="col-md-7">Frist Name</th>
                        <th class="col-md-3">Last Name</th>
                        <th class="col-md-1">Requested Roles</th>
                        <th class="col-md-1">Audition Number</th>
                    </tr>
                    </thead>
                    <tbody>
                    {{#each model.productionMembers.results as | productionMember | }}
                        <tr>
                            <td>{{productionMember.familyMemberFirstName}}</td>
                            <td>{{productionMember.familyMemberLastName}}</td>
                            <td>{{productionmember.requestedRoles}}</td>
                            <td>{{productionMember.auditionNumber}}</td>
                        </tr>
                    {{/each}}
                    </tbody>
                </table>
            </div>
        </tbody>
</div>



<div class="row">
    <div class="col-md-7">
        <div class="row">

            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">{{model.productionMember.familyMemberFirstName}} {{model.productionMember.familyMemberLastName}}</h3>
                </div>
            </div>
        </div>
    </div>

</div>
